{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },

  "extends": ["airbnb", "plugin:react/recommended"],
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
    "import/no-unresolved": "off",
    "import/extensions": 0,
    "no-shadow": "off",
    "arrow-parens": ["error", "as-needed"],
    "camelcase": "off",
   "jsx-a11y/href-no-hash": "off",
    "no-dupe-keys": "off",
    "jsx-a11y/img-has-alt": [0]
  }
}
